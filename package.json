  },
  "homepage": "https://korilakkuma.github.io/LIFE-with-FAVORITES/",
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "shallowequal": "^1.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
