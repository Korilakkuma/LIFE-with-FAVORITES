{
  "name": "life-with-favorites",
  "version": "0.0.0",
  "description": "My main website",
  "main": "./app.js",
  "scripts": {
    "clean": "rm -rf assets/app.*",
    "lint:js": "eslint src",
    "lint:css": "stylelint src",
    "lint": "run-p lint:*",
    "fix:js": "eslint --fix src",
    "fix:css": "stylelint --fix src",
    "fix": "run-p fix:*",
    "build": "npm run clean && webpack --progress --mode production",
    "dev": "webpack-dev-server --progress"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix"
    ],
    "src/**/*.css": [
      "stylelint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Korilakkuma/LIFE-with-FAVORITES.git"
  },
  "author": "Korilakkuma <rilakkuma.san.xjapan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Korilakkuma/LIFE-with-FAVORITES/issues"
  },
  "homepage": "https://korilakkuma.github.io/LIFE-with-FAVORITES/",
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "shallowequal": "^1.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "css-loader": "^5.0.2",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "eslint": "^7.20.0",
    "eslint-cli": "^1.1.1",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.3.7",
    "npm-run-all": "^4.1.5",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^5.0.0",
    "stylelint": "^13.10.0",
    "stylelint-config-standard": "^20.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.22.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  }
}
