{
  "name": "life-with-favorites",
  "version": "0.0.0",
  "description": "My main website",
  "main": "./app.js",
  "scripts": {
    "lint:js": "eslint ./src/*.js ./src/**/*.js",
    "lint:css": "stylelint ./src/*.css ./src/**/*.css",
    "lint": "run-p lint:js lint:css",
    "build": "webpack --mode production",
    "build:watch": "webpack --watch --mode development",
    "docker:build": "docker build -t app .",
    "docker:rmi": "docker rmi app",
    "start": "docker container run --name webserver -d -p 3000:80 -v $(pwd):/usr/share/nginx/html app",
    "stop": "docker stop webserver && docker rm webserver"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Korilakkuma/LIFE-with-FAVORITES.git"
  },
  "author": "Korilakkuma <rilakkuma.san.xjapan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Korilakkuma/LIFE-with-FAVORITES/issues"
  },
  "homepage": "https://korilakkuma.github.io/LIFE-with-FAVORITES/",
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "shallowequal": "^1.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^5.0.1",
    "eslint-cli": "^1.1.1",
    "eslint-plugin-react": "^7.10.0",
    "mini-css-extract-plugin": "^0.4.1",
    "npm-run-all": "^4.1.3",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.1.5",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8"
  }
}
